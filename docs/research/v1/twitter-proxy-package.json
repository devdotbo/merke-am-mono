{
  "name": "twitter-x-proxy-server",
  "version": "1.0.0",
  "description": "Multi-provider Twitter/X proxy server with 0G Storage integration",
  "type": "module",
  "main": "twitter-proxy-implementation.js",
  "scripts": {
    "start": "node twitter-proxy-implementation.js",
    "dev": "nodemon twitter-proxy-implementation.js",
    "test": "tap test/*.test.js",
    "lint": "eslint .",
    "docker:build": "docker build -t twitter-proxy .",
    "docker:run": "docker run -p 3000:3000 --env-file .env twitter-proxy"
  },
  "dependencies": {
    "fastify": "^4.26.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/cors": "^9.0.1",
    "playwright": "^1.41.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "opossum": "^8.1.3",
    "p-retry": "^6.2.0",
    "apify-client": "^2.9.3",
    "ioredis": "^5.3.2",
    "merkletreejs": "^0.3.11",
    "crypto-js": "^4.2.0",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "tap": "^18.6.1",
    "eslint": "^8.56.0",
    "@types/node": "^20.11.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "twitter",
    "x",
    "api",
